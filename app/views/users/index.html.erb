  <%= render 'books/list', user: current_user,book: @new_book %>
  
    <table class="table table-striped">
      <thead>
        <tr>
         <th>image</th>
         <th>name</th>
         <th></th>
        </tr>
      </thead>
      
      <tbody>
        <% @users.each do |user| %>
      <tr>
       <td><%= image_tag user.get_profile_image(100,100) %></td>
       <td><%= user.name %></td>
       <td><%= link_to 'show',user_path(user.id) %></td>
      </tr></br>
       <% end %>
      </tbody>
    
    </table>

